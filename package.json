{
  "name": "money",
  "version": "0.0.1",
  "description": "Money Manager",
  "main": "./server/index.js",
  "repository": "https://github.com/dimonnwc3/money",
  "scripts": {
    "nodemon": "NODE_ENV=development NODE_TRACE=true nodemon ./server/",
    "bs": "browser-sync start --config ./config/bs-config.js",
    "clean": "rm -rf ./public",
    "mocha:server": "NODE_ENV=testing NODE_TRACE=true mocha --opts ./mocha.opts ./server/test/",
    "mocha": "npm run mocha:server",
    "test": "npm run mocha:server",
    "jscs:fix": "jscs --config ./.jscsrc --fix",
    "lint": "jshint --config ./.jshintrc . && jscs --config ./.jscsrc .",
    "db:load": "NODE_ENV=development NODE_TRACE=true node ./server/fixtures/loadFixtures",
    "webpack:build:dev": "NODE_BSS=true webpack --config ./config/webpack.js",
    "webpack:build:prod": "NODE_ENV=production webpack --config ./config/webpack.js",
    "webpack:server": "webpack-dev-server --config ./config/webpack.js & npm run nodemon",
    "build:dev": "npm run webpack:build:dev",
    "build:prod": "npm run webpack:build:prod",
    "dev": "npm run lint && (npm run build:dev & npm run nodemon)",
    "devs": "npm run lint && npm run webpack:server"
  },
  "keywords": [
    "money",
    "manager"
  ],
  "author": "Dmitrii Solovev",
  "license": "ISC",
  "devDependencies": {
    "assets-webpack-plugin": "^3.4.0",
    "autoprefixer-loader": "^3.2.0",
    "babel-core": "^6.7.4",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.6.0",
    "browser-sync": "^2.11.2",
    "browser-sync-webpack-plugin": "^1.0.1",
    "clarify": "^1.0.5",
    "co-mocha": "^1.1.2",
    "co-request": "^1.0.0",
    "css-loader": "^0.23.1",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.8.5",
    "jade-loader": "^0.8.0",
    "jshint": "^2.9.1",
    "less": "^2.6.1",
    "less-loader": "^2.2.3",
    "mocha": "^2.4.5",
    "rimraf": "^2.5.2",
    "should": "^8.3.0",
    "style-loader": "^0.13.1",
    "trace": "^2.3.0",
    "url-loader": "^0.5.7",
    "webpack": "^1.12.14",
    "webpack-dev-server": "^1.14.1"
  },
  "dependencies": {
    "bootstrap": "^3.3.6",
    "co": "^4.6.0",
    "co-busboy": "^1.3.1",
    "config": "^1.19.0",
    "jade": "^1.11.0",
    "koa": "^1.2.0",
    "koa-bodyparser": "^2.0.1",
    "koa-favicon": "^1.2.1",
    "koa-generic-session": "^1.10.2",
    "koa-logger": "^1.3.0",
    "koa-passport": "^1.3.0",
    "koa-router": "^5.4.0",
    "koa-session-mongoose": "^1.0.0",
    "koa-static": "^2.0.0",
    "moment": "^2.12.0",
    "mongoose": "^4.4.11",
    "mongoose-unique-validator": "^1.0.2",
    "mz": "^2.4.0",
    "passport-local": "^1.0.0",
    "validator": "^5.2.0"
  }
}
