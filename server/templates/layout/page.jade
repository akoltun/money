include head
  body
    if flash
      if flash.success
        p.bg-success=flash.message
      if flash.success === false
        p.bg-danger=flash.message
    section.dashboard.container-fluid: .row
      include ../partials/sidebarLeft.jade
      block content
    footer.container-fluid
      block footer
    script(src='#{assets.main.js}')
    block scripts